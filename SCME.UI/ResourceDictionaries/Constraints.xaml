    <system:Single x:Key="MaxRampOpeningTime_SL">5000</system:Single>
    <system:Single x:Key="MinRampOpeningTime_SL">50</system:Single>

    <system:Single x:Key="MaxSinusTime_SL">15000</system:Single>
    <system:Single x:Key="MinSinusTime_SL">4000</system:Single>

    <system:Single x:Key="MaxCurveFactor_SL">100</system:Single>
    <system:Single x:Key="MinCurveFactor_SL">1</system:Single>

    <system:Single x:Key="MaxCurveAddTime_SL">5000</system:Single>
    <system:Single x:Key="MinCurveAddTime_SL">0</system:Single>

    <system:Single x:Key="MaxCurrentAmplitudeMax_TOU">1250</system:Single>
    <system:Single x:Key="MinCurrentAmplitudeMax_TOU">160</system:Single>

    <system:Single x:Key="MaxItm_IH_GATE">2000</system:Single>
    <system:Single x:Key="MinItm_IH_GATE">500</system:Single>

    <system:Single x:Key="MaxCalibrationCurrent_GATE">65535</system:Single>
    <system:Single x:Key="MinCalibrationCurrent_GATE">0</system:Single>

    <system:Single x:Key="MaxVoltage_DVDT">10000</system:Single>
    <system:Single x:Key="MinVoltage_DVDT">0</system:Single>

    <system:Single x:Key="MaxVoltageRateOffSet_DVDT">2000</system:Single>
    <system:Single x:Key="MinVoltageRateOffSet_DVDT">50</system:Single>

    <system:Single x:Key="MaxConfirmationCount_DVDT">100</system:Single>
    <system:Single x:Key="MinConfirmationCount_DVDT">1</system:Single>

    <system:Single x:Key="MaxVoltageRateLimit_DVDT">2500</system:Single>
    <system:Single x:Key="MinVoltageRateLimit_DVDT">500</system:Single>

    <system:Single x:Key="MaxPrePulseValue_ATU">250</system:Single>
    <system:Single x:Key="MinPrePulseValue_ATU">15</system:Single>

    <system:Single x:Key="MaxPowerValue_ATU">75</system:Single>
    <system:Single x:Key="MinPowerValue_ATU">2</system:Single>



</ResourceDictionary>