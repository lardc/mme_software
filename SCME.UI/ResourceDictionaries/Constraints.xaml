<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- Static losses parameters -->
    <system:Single x:Key="MaxITM_SL">12500</system:Single>
    <system:Single x:Key="MinITM_SL">150</system:Single>

    <system:Single x:Key="MaxTimeITM_SL">10000</system:Single>
    <system:Single x:Key="MinTimeITM_SL">10000</system:Single>

    <system:Single x:Key="MaxPulsesITM_SL">10</system:Single>
    <system:Single x:Key="MinPulsesITM_SL">1</system:Single>

    <!-- Blocking voltage parameters -->
    <system:Single x:Key="MaxIxRM_BVT">300</system:Single>
    <system:Single x:Key="MinIxRM_BVT">0.5</system:Single>
    
    <system:Single x:Key="MaxVxRM_BVT">8000</system:Single>
    <system:Single x:Key="MinVxRM_BVT">500</system:Single>
    
    <system:Single x:Key="MaxPlateVxRM_BVT">10000</system:Single>
    <system:Single x:Key="MinPlateVxRM_BVT">100</system:Single>

    <system:Single x:Key="MaxRampVxRM_BVT">5</system:Single>
    <system:Single x:Key="MinRampVxRM_BVT">0.5</system:Single>

    <system:Single x:Key="MaxStartVxRM_BVT">3000</system:Single>
    <system:Single x:Key="MinStartVxRM_BVT">300</system:Single>

    <!-- Clamping system parameters -->
    <system:Single x:Key="MaxForceCS">100</system:Single>
    <system:Single x:Key="MinForceCS">5</system:Single>

    <system:Single x:Key="MaxTempCS">200</system:Single>
    <system:Single x:Key="MinTempCS">0</system:Single>

    <system:Single x:Key="MaxHeightCS">80</system:Single>
    <system:Single x:Key="MinHeightCS">0</system:Single>


    <system:Single x:Key="MaxDirectCurrent_QRRTQ">500</system:Single>
    <system:Single x:Key="MinDirectCurrent_QRRTQ">50</system:Single>

    <system:Single x:Key="MaxDCPulseWidth_QRRTQ">5000</system:Single>
    <system:Single x:Key="MinDCPulseWidth_QRRTQ">500</system:Single>

    <system:Single x:Key="MaxDCRiseRate_QRRTQ">5</system:Single>
    <system:Single x:Key="MinDCRiseRate_QRRTQ">0.2</system:Single>

    <system:Single x:Key="MaxOffStateVoltage_QRRTQ">5</system:Single>
    <system:Single x:Key="MinOffStateVoltage_QRRTQ">0.2</system:Single>

    <system:Single x:Key="MaxRampTime_SL">10000</system:Single>
    <system:Single x:Key="MinRampTime_SL">200</system:Single>

    <system:Single x:Key="MaxRampOpeningCurrent_SL">1000</system:Single>
