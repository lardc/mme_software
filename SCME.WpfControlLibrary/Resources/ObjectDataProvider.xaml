                     xmlns:sys="clr-namespace:System;assembly=mscorlib">
    
    
    <ObjectDataProvider x:Key="VtmTestTypes" MethodName="GetVTMTestTypes" ObjectType="{x:Type dataProviders:TestTypeEnumDictionary}"/>
